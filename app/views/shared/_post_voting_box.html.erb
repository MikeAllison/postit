<!-- Called from 'posts/_post' -->
<div class="well text-center">
  <p>
    <%= post_voting_button(obj, true, 'btn-lg', 'thumbs-up', 'text-success') %>
  </p>
  <p>
    <h4><span class="label label-primary"><%= tally_votes(obj) %></span></h4>
  </p>
  <p>
    <%= post_voting_button(obj, false, 'btn-lg', 'thumbs-down', 'text-danger') %>
  </p>
</div>

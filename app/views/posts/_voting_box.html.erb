<!-- Called from 'posts/_post' -->
<div class="well text-center post-voting">
  <p>
    <%= voting_button(obj, true, 'thumbs-up', 'text-success') %> <!-- AppHelper -->
  </p>
  <h4><span class="label label-primary"><%= "#{obj.tallied_votes} Votes" %></span></h4>
  <p>
    <%= voting_button(obj, false, 'thumbs-down', 'text-danger') %> <!-- AppHelper -->
  </p>
</div>

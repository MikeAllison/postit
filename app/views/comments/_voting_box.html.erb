<!-- Called from 'comments/_comment' -->
<div id="<%= dom_id(obj, :vote) %>" class="col-sm-2 hidden-xs well text-center">
  <p>
    <%= voting_button(obj, true, 'btn-md', 'thumbs-up', 'text-success') %> <!-- AppHelper -->
    <%= voting_button(obj, false, 'btn-md', 'thumbs-down', 'text-danger') %> <!-- AppHelper -->
  </p>
  <h4><span class="label label-primary"><%= "#{obj.tallied_votes} Votes" %></span></h4>
</div>
